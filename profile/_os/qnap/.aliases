# shellcheck shell=bash
if [ "$DEBUG" = "true" ]; then echo "$DOTFILESLOC/profile/_os/qnap/.aliases" ; fi

#alias _custom='cd /share/CACHEDEV1_DATA/custom/'
#alias _share='cd /share/CACHEDEV1_DATA/'
alias _rclone='rclone rcd --rc-web-gui --rc-addr=localhost:5573 --rc-user=admin --rc-pass=admin'
alias grep='grep'
alias which='which'
alias fix_nvidia_drivers='sudo /share/CACHEDEV1_DATA/.qpkg/NVIDIA_GPU_DRV/NVIDIA_GPU_DRV.sh stop && sudo /share/CACHEDEV1_DATA/.qpkg/NVIDIA_GPU_DRV/NVIDIA_GPU_DRV.sh start'

if [[ "$ARCH" == "armv7l" ]]; then
    alias docker-cli="$DOTFILESLOC/profile/bin/docker-cli-linux-arm64"
else
    alias docker-cli="$DOTFILESLOC/profile/bin/docker-cli-linux-x64"
fi